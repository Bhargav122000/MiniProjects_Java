package other;

public class Letter {
	public static void main(String []args) {
		int i,j;
		for(i=0;i<7;i++) {
			for(j=0;j<5;j++) {
				if(i==0 && j==2)	System.out.print(".");
				if(i==1 && (j==1 || j==3))	System.out.print(".");
				if((i==2 || i==4 || i==5 || i==6) && (j==0 || j==4))	System.out.print(".");
				if(i==3)	System.out.print(".");
			}
			System.out.println();
		}
	}
}
