package other;
import java.util.*;

public class Letter_ {
	Scanner ip = new Scanner(System.in);
	char c;
	
	public Letter_() {
		System.out.println("Enter a character:");
		c = ip.nextLine().charAt(0);
	}
	
	public void display() {
		display_0();
		display_1();
		display_2();
		display_3();
		display_4();
	}
	
	public void display_0() {
		int j;
		for(j=0;j<5;j++) {
			if(c=='A' || c=='M' || c=='O' || c=='Q') {
				if(j==1 || j==2 || j==3)	System.out.print(String.format("%1s",""));
			}
		}
		
	}
	
	public void display_1() {
		
	}
	
	public void display_2() {
		
	}
	
	public void display_3() {
		
	}
	
	public void display_4() {
		
	}
	
	public static void main(String []args) {
		Letter_ obj = new Letter_();
		obj.display();
	}
}
